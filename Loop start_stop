//do while
#include<stdio.h>
int main(){
    int i=0, sum, start,stop;
    printf("enter where to start:");
    scanf("%d", start);
    printf("enter where to stop:");
    scanf("%d", stop);
    do{
    printf("%d \n",i);
    i++;
    sum+=i; 
     
     }while(i<=start,stop);
     printf("the sum is %d ", sum);
     
     return 0;
     }
